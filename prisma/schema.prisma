  competencies          Competency[]

  @@index([adminEmail], map: "idx_accounts_email")
  @@index([subscriptionTier], map: "idx_accounts_tier")
  // üåç AGREGAR ESTOS 3 √çNDICES:
  @@index([country], map: "idx_accounts_country")
