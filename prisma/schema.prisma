  reportLinkExpirationDays Int     @default(30) @map("report_link_expiration_days")
  enableEmployeeReports    Boolean @default(true) @map("enable_employee_reports")

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

