// TABLA 1: Cuentas empresa (multi-tenancy)
model Account {
  id               String        @id @default(cuid())
  companyName      String        @map("company_name")
  companyLogo      String?       @map("company_logo")
  industry         String?
  companySize      String?       @map("company_size")
